{
  "name": "woobiebot",
  "version": "0.0.0",
  "description": "Discord bot for file serving with search and rate limiting",
  "type": "module",
  "main": "./dist/index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "build": "tsc",
    "check": "tsc --noEmit && biome check . && prettier --check . && pnpm test:coverage",
    "check:fix": "tsc --noEmit && biome check --write . && prettier --write . && pnpm test:coverage",
    "dev": "pnpm build && pnpm start",
    "format:fix": "prettier --write .",
    "format": "prettier --check .",
    "lint:fix": "biome check --write .",
    "lint": "biome check .",
    "prepare": "lefthook install || true",
    "start": "node dist/index.js",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest --run",
    "test:ui": "vitest --ui",
    "test": "vitest"
  },
  "dependencies": {
    "@cap.js/server": "^4.0.2",
    "@fastify/formbody": "^8.0.2",
    "@fastify/static": "^8.2.0",
    "chokidar": "^4.0.3",
    "discord.js": "^14.23.2",
    "dotenv": "^17.2.3",
    "fast-glob": "^3.3.3",
    "fastify": "^5.6.1",
    "fuse.js": "^7.1.0",
    "mime-types": "^3.0.1",
    "pino": "^10.0.0",
    "pino-pretty": "^13.1.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@evilmartians/lefthook": "^1.13.6",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.7.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
